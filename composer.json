    "description": "This is my testing package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Test\\TestPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garage\\TestPackage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
